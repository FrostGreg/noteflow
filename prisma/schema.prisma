// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Note {
  id String @id @default(uuid())
  date DateTime @default(now())
  name String
  title String
  length String
  imagePath String
  chips String
  content String
}